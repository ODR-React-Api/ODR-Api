<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.app.mapper.GetMediatorDisclosureDateMapper">
    <select id="getMediatorDisclosureDate" parameterType="string" resultType="date">
        SELECT MediatorDisclosureDate 
            from cases
        WHERE
            cid = #{caseId}
    </select>
</mapper>
