<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.app.mapper.LanguagesMapper">

    <resultMap id="BaseResultMap" type="com.web.app.domain.Languages">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="languageName" column="LanguageName" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="selectAllLanguages" resultMap="BaseResultMap">
        select id,languageName from react_use.master_languages where deleteFlag = '0'
    </select>

</mapper>
