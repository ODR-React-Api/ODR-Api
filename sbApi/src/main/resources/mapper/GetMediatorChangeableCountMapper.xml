<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.app.mapper.GetMediatorChangeableCountMapper">
    <select id="SelCases" resultType="com.web.app.domain.Entity.Cases">
        SELECT
            CaseStage,
            MediatorChangeableCount1,
            MediatorChangeableCount2
        FROM
            cases
        WHERE
            cid = #{CaseId}
    </select>
</mapper>