<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.app.mapper.RegisterUserMapper">

    <insert id="RegisterUserMapper">
        insert into odr_users(Uid,Email,FirstName,MiddleName,LastName,FirstName_kana,MiddleName_kana,LastName_kana,CompanyName,PlatformId,LanguageId) 
        values(#{Uid},#{Email},#{FirstName},#{MiddleName},#{LastName},#{FirstName_Kana},#{MiddleName_Kana},#{LastName_Kana},#{CompanyName},#{PlatformId},#{LanguageId})
    </insert>

</mapper>
