<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.app.mapper.GetMediationsTemplateMapper">
    <select id="findMediationsTemplate" resultType="com.web.app.domain.MediationsConCon.MediationsTemplate">
        SELECT 
            Context
        FROM
            master_templates
        WHERE
            platformId = #{platformId} and
            DeleteFlag = 0 and
            templateType = #{templateType} and
            LanguageId = #{languageId} 
    </select>
</mapper>