<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.app.mapper.">

    <select id="" resultType="">
        SELECT 
            HtmlContext,
            HtmlContext2,
            AgreementDate,
            Status
        FROM
            case_negotiations
        WHERE 
            CaseId = #{CaseId}
            AND DeleteFlag = #{DeleteFlag}
    </select>

</mapper>