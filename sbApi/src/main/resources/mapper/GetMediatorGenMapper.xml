<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.web.app.mapper.GetMediatorGenMapper">
    <resultMap id="MediateUserMap" type="com.web.app.domain.MedUserConfirm.GetMediatorGen">
        <result property="userId" column="userId" jdbcType="VARCHAR"/>
        <result property="Status" column="Status" jdbcType="INTEGER"/>
    </resultMap>
 
    <select id="getMediatorGen" resultMap="MediateUserMap">
        SELECT
            userId,
            Status
        FROM
            mediator_histories
        WHERE
            CaseId = #{CaseId}
            AND Status in (0, 1, 3, 6)
    </select>
</mapper>